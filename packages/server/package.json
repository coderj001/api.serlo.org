    "apollo-server-core": "^3.9.0",
    "apollo-server-express": "^3.9.0",
    "apollo-server-plugin-base": "^3.6.1",
    "apollo-server-types": "^3.6.1",
    "basic-auth": "^2.0.1",
    "bee-queue": "^1.4.0",
