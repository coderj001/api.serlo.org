    "graphql-type-json": "^0.3.2",
    "io-ts": "^2.2.16",
    "io-ts-reporters": "^2.0.1",
    "io-ts-types": "^0.5.16",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.8.0",
