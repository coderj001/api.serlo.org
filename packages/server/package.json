    "@types/jsonwebtoken": "^8.5.8",
    "@types/node-fetch": "^2.6.1",
    "@types/ramda": "^0.28.12",
    "@types/redis": "^2.8.32",
    "@types/redlock": "^3.0.7",
    "@types/semver": "^7.3.9",
    "@types/uuid": "^8.3.4",
